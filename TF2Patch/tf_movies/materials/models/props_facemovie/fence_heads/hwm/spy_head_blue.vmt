"VertexLitGeneric"

{
	"$basetexture" "models/props_facemovie/fence_heads/hwm/spy_head_blue"
	"$basemapalphaphongmask" "1"
	// wrinkle maps need to have $bumpmap enabled to show up
	"$compress"    "models/props_facemovie/fence_heads/hwm/spy_head_blue_compress"
	"$stretch"     "models/props_facemovie/fence_heads/hwm/spy_head_blue_stretch"
	// $bumpmap is a shared 16x16 flat normal map, and is needed or lightwarp won't shade correctly.
	"$bumpmap"     "models/player/shared/shared_normal"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "1"
	"$detailblendfactor" .01
	"$detailblendmode" 6


	"$bbqamount" "1"	
	"$two" "2.35"
	"$one" "1"
	"$absburnminusone" "1"
	"$burnminusone" "1"
	"$finalburnlevel" "1"
	"$burnlevel" "0"


	"$phong" "1"
	"$phongexponenttexture" "models/props_facemovie/fence_heads/hwm/spy_head_exponent"
	"$phongboost" "[.05]"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" ".5"										// Exponent for phong component of rim lighting	
	"$rimlightboost" ".5"										// Boost for ambient cube component of rim lighting
	"$rimmask" "1"                  // Use the alpha channel of the $phongexponenttexture as a rim mask.
	
	// Cloaking
	"$cloakPassEnabled" "1"
	"$cloakColorTint" "[0.4 0.5 1]"
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$burnlevel"
		}
		"Subtract"
		{
			"srcVar1" "$burnlevel"
			"srcVar2" "$one"
			"resultVar" "$burnminusone"
		}
		"Abs"
		{
			"srcVar1" "$burnminusone" 
			"resultVar" "$absburnminusone"
		}

		"Subtract"
		{
			"srcVar1" "$one"
			"srcVar2" "$absburnminusone"
			"resultVar" "$finalburnlevel"
		}


		"Equals"
		{
			"srcVar1" "$finalburnlevel"
			"resultVar" "$detailblendfactor"
		}
	

	}
}

